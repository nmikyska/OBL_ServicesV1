<mjml>
  <mj-head>
    <mj-style inline="inline">
      .bold {
        font-weight: 800 !important;
        color: #e43c03 !important;
        text-transform: uppercase !important;
      }
      h2{
        margin-top: 10px !important;
        margin-bottom: 10px !important;
      }
      .link{
        color: #ffffff !important;
        text-decoration: none !important;
      }
      .link:hover{
        color: #ffffff !important;
        text-decoration: none !important;
      }
      .learn{
        color: #000000 !important;
        text-decoration: none !important;
        border: 1px solid #000000 !important;
        padding-top: 5px !important;
        padding-bottom: 5px !important;
        padding-right: 10px !important;
        padding-left: 10px !important;
      }
      .learn:hover{
        background-color: #000000 !important;
        color: #ffffff !important;
        text-decoration: none !important;
        border: 1px solid #000000 !important;
        padding-top: 5px !important;
        padding-bottom: 5px !important;
        padding-right: 10px !important;
        padding-left: 10px !important;
      }
      
    </mj-style>
    <!--preview-->
    <mj-preview>OBL Financial </mj-preview>
    <mj-title>OBL Financial</mj-title>
  </mj-head>
  <mj-body>
    <!--start header logo section-->
      <mj-section background-color="#ffffff">
        <mj-column>
          <mj-image width="600px" src="http://www.nmikyska.com/emails/obl1/logo-lg.jpg" />
        </mj-column>
      </mj-section>
    <!--end header logo section-->
    <!--start of header nav section-->

      <mj-section background-color="#013475">
        <mj-column>
          <mj-navbar base-url="https://www.oblfinancial.com" hamburger="hamburger" ico-color="#ffffff">
              <mj-navbar-link href="https://www.oblfinancial.com/programs" color="#ffffff">Programs</mj-navbar-link>
              <mj-navbar-link href="https://www.oblfinancial.com/services" color="#ffffff">services</mj-navbar-link>
              <mj-navbar-link href="https://www.oblfinancial.com/solutions" color="#ffffff">Solutions</mj-navbar-link>
              <mj-navbar-link href="https://www.oblfinancial.com/aplly" color="#ffffff">Apply</mj-navbar-link>
          </mj-navbar>
        </mj-column>
      </mj-section>
    <!--end of header nav section-->
    <!--start of hero image-->
      <mj-wrapper padding-top="0px" padding-bottom="0px">
        <mj-hero
          mode="fluid-height"
          background-width="600px"
          background-height="400px"
          background-url="http://www.nmikyska.com/emails/obl1/officeGroup.jpg"
          background-color="#2a3448"
          padding="100px 0px"
          >
        <!-- To add content like mj-image, mj-text, mj-button ... use the mj-hero-content component -->
          <mj-text
            padding="15px"
            color="#ffffff"
            font-family="Helvetica"
            align="center"
            font-size="30px"
            line-height="35px"
            font-weight="400">
            In a world of computers...<br /> We're <span class="bold"> real </span>people. Investing in <span class="bold">you</span>.
          </mj-text>
        <mj-button href="https://www.oblfinancial.com/" align="center" background-color="#e43c03" font-size="20px" line-height="26px" padding-top="30px" text-transform="uppercase">
        Click or call <br />
          630-850-7180
        </mj-button>
      </mj-hero>
    </mj-wrapper>
  <!--end of hero image-->
  <!--start of image/text section-->
    <mj-section padding-top="10px"    padding-left="0px" padding-left="0px" background-color="#ffffff">
    <mj-group>
      <mj-column width="40%" padding="0px 0px 0px 0px">
        <mj-image width="600px" align="left" padding="0px 10px 0px 0px" src="http://www.nmikyska.com/emails/obl1/TruckingServices.jpg" alt="truck" />
      </mj-column>
      
      <mj-column width="60%" padding="0px">
        <mj-text align="left" padding="0px 10px 0px 0px" font-size="16px" line-height="16px">
          <h2>Trucking</h2>
          </mj-text>
          <mj-text align="left" padding="0px 10px 0px 0px" font-size="14px" line-height="17px">
          <p>OBL Financial has been financing Owner Operators and Small Fleets, both new and experienced, since 1999. </p>
          
          <p>Working with lenders to approve and fund your truck and trailer purchases.</p>
          <p><a href="https://www.#.com" class="learn">Learn More</a></p>
         </mj-text>
         
      </mj-column>
      </mj-group>
    </mj-section>
  <!--end of 1st img/text section-->
  <!--start of divider-->
   <mj-section background-color="#ffffff">
